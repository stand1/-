.choseType .type {
  font-size: 29rpx;
  font-weight: 600;
  color: #999999;
  text-align: center;
  margin: 46rpx 0;
}

.choseType .title {
  color: #9B9B9B;
  font-size: 24rpx;
  margin: 0 34rpx;
}

.choseType .block {
  display: flex;
  align-items: center;
  padding: 23rpx 50rpx;
  margin-bottom: 32rpx;
}

.choseType .block image {
  width: 211rpx;
  height: 96rpx;
  margin-right: 18rpx;
}

.choseType .rt .carTit {
  font-size: 30rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #000000;
}

.choseType .rt .desp {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #9B9B9B;
}

.car .task {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
}

.car .box {
  position: fixed;
  right: 0;
  width: 574rpx;
  height: 100%;
  background: #fff;
  top: 0;
  z-index: 2;
}

.car .box {
  padding: 34rpx 38rpx;
}

.car .box .btn {
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  color: #9B9B9B;

}

.car .sure {
  color: #3366FF;
}

.car .tit {
  margin: 36rpx 0 12rpx 0;
  font-size: 40rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #000000;
}

.car .ctit {
  margin-bottom: 72rpx;
  font-size: 24rpx;
  color: #9B9B9B;
}

.alltit image {
  width: 48rpx;
  height: 48rpx;
}

.alltit {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx 0;
}

.alltit text,
.select__nav .name {
  font-size: 30rpx;
  font-weight: 600;
  color: #000000;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  flex: 1;
  width: 100%;
}

.select__nav .block {
  margin-top: 72rpx;
}

.select__nav {
  overflow-y: auto;
}