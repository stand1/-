/* pages/cropper/cropper.wxss */
page{
  
}
.cropper{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.cropper-wrapper{
  height: 100vh;
  width: 100%;
  background: #000;
  position: relative;
}
.cropper-buttons{
  width: 140rpx;
  height: 64rpx;
  line-height: 64rpx;
  background-color: #f81e36;
  color: #fff;
  border-radius: 56rpx;
  font-size: 28rpx;
  text-align: center;
  position: absolute;
  bottom: 120rpx;
  left: calc(50% - 70rpx);
  z-index: 99;
}