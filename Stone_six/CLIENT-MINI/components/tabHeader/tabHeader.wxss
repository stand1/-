/* components/tabHeader/tabHeader.wxss */
.tabHeader{
  width: 100%;
  box-sizing: border-box;
  /* padding-top: 40rpx; */
  background-color: #fff;
}
.tabHeader .content{
  width: 100%;
  height: 80rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  background-color: #fff;
  /* padding: 24rpx 0; */
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
}
.contentLeft{
  display: flex;
  /* justify-content: center; */
  align-items: center;
  background-image: url("../../static/tabs/iconBg.png") no-repeat;
  background-size: 100% 100%;
  width: 174rpx;
  height: 62rpx;
  /* padding: 12rpx 24rpx; */
  box-sizing: border-box;
  margin-right: 124rpx;
  padding-left: 26rpx;
}
.contentLeft image{
  width: 40rpx;
  height: 40rpx;
}
.contentLeft .back{
  margin-right: 40rpx;
}
.contentTxt{
  font-size: 34rpx;
  font-family: Helvetica;
  color: #000000;
  display: flex;
  /* font-weight: 600; */
}
.contentTxt image{
  width: 18rpx;
  height: 12rpx;
  margin-left: 20rpx;
  margin-bottom: 3rpx;
}